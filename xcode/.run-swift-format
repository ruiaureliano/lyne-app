#!/bin/sh
cd "$(dirname "$0")"
echo "START FORMATING CODE\n"
swift-format -r -i ./Lyne/Source  --configuration ./.swift-format
echo "   游릭 Lyne/Source"
swift-format -r -i ./Lyne\ \(Paddle\)/Source  --configuration ./.swift-format
echo "   游릭 Lyne (Paddle)/Source"
swift-format -r -i ./Lyne\ \(Setapp\)/Source  --configuration ./.swift-format
echo "   游릭 Lyne (Setapp)/Source"
swift-format -r -i ./Lyne\ \(Apple\)/Source  --configuration ./.swift-format
echo "   游릭 Lyne (Apple)/Source"